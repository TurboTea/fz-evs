generator client {
  provider = "prisma-client-js"
  engineType = "library"
  binaryTargets = ["native", "windows"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}
model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  jobTitle String
}
