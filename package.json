{
  "name": "fz-evs",
  "version": "0.1.5",
  "private": true,
  "author": "gxkai",
  "scripts": {
    "prebuild": "concurrently \"rollup -c\"  \"vue-cli-service build --mode prod\"",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "lint": "vue-cli-service lint --mode dev",
    "build": "npm run prebuild && electron-builder -w",
    "serve": "concurrently \"vue-cli-service serve\" \"rollup -c && wait-on tcp:9000 && electron .\"",
    "api:generate": "ts-node scripts/api-generate.ts",
    "api:mock": "ts-node scripts/api-mock.ts",
    "dev": "yarn serve & yarn api:mock",
    "font": " icon-font-generator src/assets/icons/font/*.svg -o src/assets/font --name font --prefix font",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "tailwind": "npx tailwindcss -i src/assets/css/tailwind.css -o ./tailwind.css --watch",
    "prisma:db:push": "npx prisma db push --preview-feature",
    "prisma:db:push:force": "npx prisma db push --preview-feature --accept-data-loss",
    "prisma:db:seed": "npx prisma db seed --preview-feature",
    "prisma:generate": "npx prisma generate",
    "prisma:introspect": "npx prisma introspect",
    "prisma:chain": "npm run prisma:db:push:force && npm run prisma:introspect && npm run prisma:generate && npm run serve",
    "prisma:studio": "npx prisma studio",
    "codegen:generate": "graphql-codegen --watch"
  },
  "main": "dist_main/index.js",
  "build": {
    "appId": "com.fz.app",
    "productName": "example app",
    "copyright": "Copyright Â© 2021 ${author}",
    "publish": [
      {
        "provider": "github",
        "owner": "gxkai",
        "repo": "https://github.com/TurboTea/fz-evs.git"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "files": [
      "dist/**",
      "dist_main/**",
      "electron-src/**/*",
      "node_modules/**/*",
      "build/**/*"
    ],
    "directories": {
      "buildResources": "package",
      "output": "dist_electron"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.4",
    "@prisma/client": "3.8.1",
    "@vuelidate/core": "^2.0.0-alpha.32",
    "@vuelidate/validators": "^2.0.0-alpha.25",
    "@vueuse/core": "^7.5.3",
    "axios": "0.21.1",
    "core-js": "^3.6.5",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^4.6.1",
    "element-plus": "1.3.0-beta.5",
    "fs": "^0.0.1-security",
    "global": "^4.4.0",
    "is-electron": "^2.2.1",
    "lodash": "^4.17.21",
    "pinia": "^2.0.0-rc.10",
    "pinia-plugin-persistedstate": "^1.0.2",
    "prisma": "^3.8.1",
    "tailwindcss": "^3.0.2",
    "type-fest": "^2.8.0",
    "vue": "^3.2.26",
    "vue-i18n": "^9.2.0-beta.23",
    "vue-request": "^1.2.3",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@stoplight/prism-cli": "^4.6.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/crypto-js": "^4.0.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "^5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "^5.0.0-rc.1",
    "@vue/cli-plugin-router": "^5.0.0-rc.1",
    "@vue/cli-plugin-typescript": "^5.0.0-rc.1",
    "@vue/cli-service": "^5.0.0-rc.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.4",
    "compression-webpack-plugin": "^9.0.0",
    "concurrently": "^7.0.0",
    "electron": "^13.0.0",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.0",
    "icon-font-generator": "^2.1.11",
    "image-webpack-loader": "^8.0.1",
    "lint-staged": "^12.1.2",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.4.5",
    "minimist": "^1.2.5",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "postcss": "^8.4.5",
    "postcss-focus-visible": "^6.0.2",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-nesting": "^10.0.3",
    "postcss-preset-env": "^7.0.1",
    "prettier": "^2.2.1",
    "rollup": "^2.56.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "shelljs": "^0.8.5",
    "svg-sprite-loader": "^6.0.11",
    "swagger-typescript-api": "^9.3.1",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4",
    "typescript-json-serializer": "^3.4.4",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.11",
    "wait-on": "^6.0.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  }
}
